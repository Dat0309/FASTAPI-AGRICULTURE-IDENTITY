{
    "version": 2,
    "builds": [{
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.11.3"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py",
            "methods":["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"]
        }
    ]
}